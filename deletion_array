#include<iostream>
using namespace std;
int main(){
    int arr[100],position,n;
    cin>>n;    //no of elements
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
   for (int i=0;i<n;i++){
    cout<<arr[i];
   }
   cin>>position;
   for(int i=position-1;i<n-1;i++)
   {
       arr[i]=arr[i+1];
   }
   n--;
    for (int i=0;i<n;i++){
    cout<<arr[i];
   }
   return 0; 
}
